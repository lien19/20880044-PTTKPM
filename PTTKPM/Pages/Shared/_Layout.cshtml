    @*col-resize*@
    <script>
        //create Column Resize
        var tables = document.getElementsByTagName('table')
        for (var i = 0; i < tables.length; i++) { resizableGrid(tables[i]) }

        function resizableGrid(table) {//retrieves the very first row of the table
            var row = table.getElementsByTagName('tr')[0]
            var cols = row ? row.children : undefined  //store all td elements to cols variable using row.children
            if (!cols) return

            //table.style.overflow = 'hidden'

            for (var i = 0; i < cols.length; i++) {
                var div = createDiv(table.offsetHeight) //createDiv function-create column resizer handle
                cols[i].appendChild(div)
                cols[i].style.position = 'relative'
                setListeners(div)                //setListeners function
            }

            function createDiv(height) { //create column resizer handle
                var div = document.createElement('div')
                div.style.top = 0
                div.style.right = 0
                div.style.width = '5px'
                div.style.position = 'absolute'
                div.style.cursor = 'col-resize'
                // div.style.background = 'red'
                div.style.userSelect = 'none'
                div.style.height = height + 'px'
                div.className = 'columnSelector'
                return div
            }

            function setListeners(div) {
                var pageX, curCol, nxtCol, curColWidth, nxtColWidth
                div.addEventListener('mousedown', function (e) {
                    curCol = e.target.parentElement
                    nxtCol = curCol.nextElementSibling
                    console.log(curCol)
                    pageX = e.pageX
                    curColWidth = curCol.offsetWidth
                    if (nxtCol) { nxtColWidth = nxtCol.offsetWidth }
                })
                document.addEventListener('mouseover', function (e) {
                    if (curCol) {
                        var diffX = e.pageX - pageX
                        if (nxtCol) { nxtCol.style.width = (nxtColWidth - (diffX)) + 'px' }
                        curCol.style.width = (curColWidth + diffX) + 'px'
                    }
                })
                document.addEventListener('mouseup', function (e) {
                    curCol = undefined
                    nxtCol = undefined
                    pageX = undefined
                    nxtColWidth = undefined
                    curColWidth = undefined
                })
            }
        }


    </script>

    @*popup form*@
    <script>
        var modalCreate = document.getElementById('form-popup2')
        var modalUpdate = document.getElementById('form-popup3')
        var modalDelete = document.getElementById('form-popup4')

