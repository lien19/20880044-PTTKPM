        var indexDelete = window.location.href.indexOf("delete?Id=")

        var url = window.location.href
        //... Modal hidden & URL loaded without query param ... if click on element wrapping form
        window.onclick = function (event) {
            if (event.target == modalCreate) { modalCreate.style.display = 'none' }
            else if (event.target == modalUpdate) { modalUpdate.style.display = 'none'; window.location.href = url.split("/update?Id=")[0]; }
            else if (event.target == modalDelete) { modalDelete.style.display = 'none'; window.location.href = url.split("/delete?Id=")[0]; }
        }   

        //... Modal pop-ups ... if query param is added to URL
        if (indexUpdate > -1) { modalUpdate.style.display = 'block' }
        else if (indexDelete > -1) { modalDelete.style.display = 'block' }
